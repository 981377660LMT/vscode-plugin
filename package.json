{
  "name": "create-component",
  "publisher": "cmnx",
  "displayName": "Create Component",
  "description": "快速生成React/Vue组件",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.57.0"
  },
  "categories": [
    "Other",
    "Snippets"
  ],
  "activationEvents": [
    "onCommand:extension.createReactComponent",
    "onCommand:extension.createVue2Component",
    "onCommand:extension.createVue3Component"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.createReactComponent",
        "title": "Create Component - React "
      },
      {
        "command": "extension.createVue2Component",
        "title": "Create Component - Vue2"
      },
      {
        "command": "extension.createVue3Component",
        "title": "Create Component - Vue3"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "extension.createReactComponent",
          "group": "2_workspace"
        },
        {
          "command": "extension.createVue2Component",
          "group": "2_workspace"
        },
        {
          "command": "extension.createVue3Component",
          "group": "2_workspace"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "test-compile": "tsc -p ./",
    "test-watch": "tsc -watch -p ./",
    "pretest": "npm run test-compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/ejs": "^3.0.6",
    "@types/fs-extra": "^9.0.11",
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.2.2",
    "@types/node": "14.x",
    "@types/react": "^17.0.11",
    "@types/vscode": "^1.57.0",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "clean-webpack-plugin": "^4.0.0-alpha.0",
    "copy-webpack-plugin": "^9.0.0",
    "eslint": "^7.27.0",
    "glob": "^7.1.7",
    "mocha": "^8.4.0",
    "ts-loader": "^9.2.2",
    "typescript": "^4.3.2",
    "vscode-test": "^1.5.2",
    "webpack": "^5.38.1",
    "webpack-cli": "^4.7.0"
  },
  "dependencies": {
    "ejs": "^3.1.6",
    "fs-extra": "^10.0.0",
    "react": "^17.0.2"
  }
}
